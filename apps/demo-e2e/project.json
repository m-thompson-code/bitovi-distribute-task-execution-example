{
  "name": "demo-e2e",
  "targets": {
    "generate-tests": {
      "executor": "@bitovi-distribute-task-execution-example/stress-tests:generate-tests"
    },
    "e2e": {
      "dependsOn": [
        {
          "target": "generate-tests",
          "projects": "self"
        }
      ],
      "options": {
        "passWithNoTests": false
      }
    },
    "e2e-ci": {
      "dependsOn": [
        {
          "target": "generate-tests",
          "projects": "self"
        }
      ],
      "options": {
        "passWithNoTests": false
      }
    }
  },
  "tags": ["scope:demo"],
  "implicitDependencies": ["demo"]
}
